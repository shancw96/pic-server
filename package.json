{
  "name": "koa-pic-base",
  "version": "0.0.1",
  "description": "file-server based on koa",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --ignore tests/ --watch src -e ts,tsx --exec ts-node src/index.ts",
    "build-ts": "tsc",
    "serve": "node dist/index.js",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix"
  },
  "engines": {
    "node": "14.x.x",
    "npm": "6.x.x"
  },
  "keywords": [
    "koa",
    "file-server"
  ],
  "_moduleAliases": {
    "src": "dist"
  },
  "author": "shancw",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "@koa/multer": "^3.0.0",
    "@koa/router": "^10.1.1",
    "dotenv": "^16.0.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.4",
    "koa-jwt": "^4.0.3",
    "koa-router": "^10.1.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.5.8",
    "@types/koa": "^2.13.4",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__multer": "^2.0.4",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "nodemon": "^2.0.15",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
